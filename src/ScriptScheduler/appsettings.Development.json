{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "Microsoft": "Debug",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "Hangfire": "Warning",
        "Microsoft.EntityFrameworkCore": "Debug",
        "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "CsScriptOption": {
    "ScriptPath": "D:\\workspace\\ScriptScheduler\\src\\ScriptScheduler\\bin\\Debug\\net7.0\\Script",
    "Interval": 1
  },
  "PythonScriptOption": {
    "ScriptPath": "D:\\workspace\\ScriptScheduler\\src\\ScriptScheduler\\bin\\Debug\\net7.0\\Script",
    "Interval": 1,
    "PipList":[
      "selenium",
      "pymssql",
      "requests"
    ]
  }
}